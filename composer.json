{
    "name": "idct/symfony-form-soft-delete-aware-entity-type",
    "description": "Repository which adds support for Soft Delete filter behavior to Symfony's EntityType.",
    "type": "library",
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "IDCT\\Tests\\SymfonyFormSoftDeleteAwareEntityType\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "IDCT\\SymfonyFormSoftDeleteAwareEntityType\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bartosz Pacho≈Çek",
            "email": "bartosz@idct.tech"
        }
    ],
    "scripts": {
        "fix": [
            "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --allow-risky=yes"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "gedmo/doctrine-extensions": "^3",
        "symfony/doctrine-bridge": "^6 || ^7",
        "symfony/property-access": "^6 || ^7",
        "symfony/options-resolver": "^6 || ^7",
        "symfony/form": "^6 || ^7",
        "doctrine/orm": "^2|^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.59"
    }
}
